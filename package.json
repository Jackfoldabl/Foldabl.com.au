{
  "version": "1.0.0",
  "source": "./index.html",
  "type": "module",
  "name": "anima-project",
  "description": "A React project automatically generated by Anima",
  "scripts": {
    "dev": "vite",
    "gen:sitemap": "node scripts/generate-sitemap.mjs",
    "build": "npm run gen:sitemap && vite build && npm run postbuild:conditional",
    "build:vercel": "npm run gen:sitemap && vite build",
    "postbuild": "npm run postbuild:conditional",
    "postbuild:conditional": "sh -c 'if [ \"$PRERENDER\" = \"true\" ] && command -v google-chrome-stable >/dev/null 2>&1 && npm list react-snap >/dev/null 2>&1; then react-snap; else echo Skipping prerender - Chrome, react-snap not available or PRERENDER not enabled; fi'",
    "images:convert": "node scripts/convert-images.mjs"
  },
  "dependencies": {
    "clsx": "2.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "tailwind-merge": "2.5.4",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@animaapp/vite-plugin-screen-graph": "^0.1.5",
    "@vitejs/plugin-react": "4.3.4",
    "esbuild": "0.24.0",
    "globals": "15.12.0",
    "sharp": "^0.33.5",
    "tailwindcss": "3.4.16",
    "vite": "6.0.4"
  },
  "optionalDependencies": {
    "react-snap": "^1.23.0"
  }
  ,
  "reactSnap": {
    "source": "dist",
    "destination": "dist",
    "inlineCss": true,
    "puppeteerArgs": [
      "--no-sandbox", 
      "--disable-setuid-sandbox",
      "--disable-dev-shm-usage",
      "--disable-accelerated-2d-canvas",
      "--disable-gpu",
      "--window-size=1920x1080"
    ],
    "puppeteerExecutablePath": "/usr/bin/google-chrome-stable",
    "include": [
      "/",
      "/modular-homes",
      "/blog",
      "/blog/emergency-modular-homes-australia"
    ]
  }
}
